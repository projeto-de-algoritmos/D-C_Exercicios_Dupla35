const multiply = function(num1, num2) {

  const resultArray = [...Array(num1.length + num2.length)].fill(0);

  for (let i = num1.length - 1; i >= 0; i--) {
    for (let j = num2.length - 1; j >= 0; j--) {
      const previousRemainder = resultArray[i + j + 1];
      const product = num1[i] * num2[j] + previousRemainder;
      const unitsDigit = product % 10;
      const carryOver = Math.floor(product / 10);
      resultArray[i + j + 1] = unitsDigit;
      resultArray[i + j] += carryOver;
    }
  }
  let leadingZeroesIndex = 0;
  while (resultArray[leadingZeroesIndex] === 0) {
    resultArray.shift();
  }
  
  // Se o resultado estiver vazio, retorna "0"
  if (!resultArray.length) {
    return "0";
  }
  
  // Retorna o resultado como uma string
  return resultArray.join("");
};
